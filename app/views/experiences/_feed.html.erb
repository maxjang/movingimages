<% @instagram.each do |instagram| %>
  <% if instagram.type == "video" %>
    <%= link_to instagram.videos.standard_resolution.url do %>
      <%= video_tag instagram.videos.standard_resolution.url, :controls => true %>
    <% end %><br/>
  <% else %>
    <%= link_to instagram.images.standard_resolution.url do %>
      <%= image_tag instagram.images.standard_resolution.url %>
    <% end %><br/>
  <% end %>
  <%= instagram.user.username %><br/>
  Likes: <%= instagram.likes[:count] %><br/>
  <% instagram.likes[:data].each do |user| %>
    <%= image_tag user.profile_picture %>
  <% end %><br/>
  Comments: <%= instagram.comments[:count] %><br/>
  <% instagram.comments[:data].each do |user| %>
    <%= image_tag user.profile_picture %>
  <% end %><br/>
<% end %>
